	"Atomic Pane Story": {
		"prefix": "/astory",
		"description": "",
		"body": [
			"--[[ File Info",
			"",
			"\tAuthor(s): ",
			"\tModule: $TM_FILENAME",
			"\tVersion: 1.0.0",
			"",
			"]]",
			"",
			"require(game.ReplicatedStorage.Order)",
			"",
			"local ${TM_FILENAME_BASE/^(.*)\\..*/$1/} = shared(\"${TM_FILENAME_BASE/^(.*)\\..*/$1/}\") ---@module ${TM_FILENAME_BASE/^(.*)\\..*/$1/}",
			"",
			"local Story = {",
			"\tcontrols = {",
			"\t\tVisible = true,",
			"\t}",
			"\trender = function(props)",
			"\t\tlocal pane = ${TM_FILENAME_BASE/^(.*)\\..*/$1/}.new(props.target)",
			"",
			"\t\t-- Define controls",
			"",
			"\t\tlocal controlCallbacks = {",
			"\t\t\tVisible = function(value: boolean)",
			"\t\t\t\tpane:SetVisible(value)",
			"\t\t\tend,",
			"\t\t}",
			"",
			"\t\t-- Subscribe to control changes (don't edit)",
			"",
			"\t\tlocal unsubscribe = props.subscribe(function(_, infos)",
			"\t\t\tfor control, value in pairs(infos) do",
			"\t\t\t\tif controlCallbacks[control] and value.__new ~= value.__old then",
			"\t\t\t\t\tcontrolCallbacks[control](value.__new)",
			"\t\t\t\tend",
			"\t\t\tend",
			"\t\tend)",
			"",
			"\t\t-- Initialize control values (don't edit)",
			"",
			"\t\tfor control, value in pairs(props.controls) do",
			"\t\t\tif controlCallbacks[control] then",
			"\t\t\t\tcontrolCallbacks[control](value)",
			"\t\t\tend",
			"\t\tend",
			"",
			"\t\t-- Cleanup",
			"",
			"\t\treturn function() -- Cleanup your story here",
			"\t\t\tpane:Destroy()",
			"\t\t\tunsubscribe()",
			"\t\tend",
			"\tend,",
			"}",
			"",
			"return Story",
			""
		],
